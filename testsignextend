    .scope S_0x1085c70;
T_1 ;
    %set/v v0x10ba340_0, 1, 1;
    %set/v v0x10ba280_0, 1, 1;
    %movi 8, 43690, 16;
    %set/v v0x10ba490_0, 8, 16;
    %delay 0, 0;
    %load/v 8, v0x10ba3e0_0, 32;
    %movi 40, 4294945450, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_1.0, 4;
    %set/v v0x10ba340_0, 0, 1;
    %vpi_call 2 35 "$display", "Test Case 1 Failed: MSB in imm 1 implementation failed";
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 38 "$display", "Test Case 1 Passed";
T_1.1 ;
