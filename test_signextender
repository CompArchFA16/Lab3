#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1a2a730 .scope module, "testSignextend" "testSignextend" 2 3;
 .timescale 0 0;
v0x1a71880_0 .var "clk", 0 0;
v0x1a71950_0 .var "imm", 15 0;
v0x1a71a00_0 .net "signextendOut", 31 0, v0x1a717e0_0; 1 drivers
S_0x1a2a820 .scope module, "signext" "signextender" 2 12, 3 3, S_0x1a2a730;
 .timescale 0 0;
v0x1a60580_0 .net "clk", 0 0, v0x1a71880_0; 1 drivers
v0x1a71740_0 .net "imm", 15 0, v0x1a71950_0; 1 drivers
v0x1a717e0_0 .var "signextendOut", 31 0;
E_0x1a3c7f0 .event edge, v0x1a60580_0;
    .scope S_0x1a2a820;
T_0 ;
    %wait E_0x1a3c7f0;
    %ix/load 1, 15, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.0, 4;
    %load/x1p 8, v0x1a71740_0, 1;
    %jmp T_0.1;
T_0.0 ;
    %mov 8, 2, 1;
T_0.1 ;
; Save base=8 wid=1 in lookaside.
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_0.2, 6;
    %load/v 8, v0x1a71740_0, 16;
    %mov 24, 0, 16;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1a717e0_0, 0, 8;
T_0.2 ;
    %ix/load 1, 15, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 8, v0x1a71740_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 8, 2, 1;
T_0.5 ;
; Save base=8 wid=1 in lookaside.
    %cmpi/u 8, 1, 1;
    %jmp/0xz  T_0.6, 6;
    %load/v 8, v0x1a71740_0, 16;
    %mov 24, 1, 16;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1a717e0_0, 0, 8;
T_0.6 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1a2a730;
T_1 ;
    %set/v v0x1a71880_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1a2a730;
T_2 ;
    %delay 10, 0;
    %load/v 8, v0x1a71880_0, 1;
    %inv 8, 1;
    %set/v v0x1a71880_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1a2a730;
T_3 ;
    %set/v v0x1a71880_0, 1, 1;
    %vpi_call 2 20 "$display", "-------------------------";
    %vpi_call 2 21 "$display", "SIGNEXTENDER TEST CASES";
    %vpi_call 2 22 "$display", "-------------------------";
    %vpi_call 2 26 "$display", "| imm | signextendOut |";
    %movi 8, 43690, 16;
    %set/v v0x1a71950_0, 8, 16;
    %delay 1000000, 0;
    %vpi_call 2 28 "$display", "| \011%b \011\011| \011\011%b \011\011| ", v0x1a71950_0, v0x1a71a00_0;
    %vpi_call 2 31 "$display", "| imm | signextendOut |";
    %movi 8, 21845, 16;
    %set/v v0x1a71950_0, 8, 16;
    %delay 100, 0;
    %vpi_call 2 33 "$display", "| \011%b \011\011| \011\011%b \011\011| ", v0x1a71950_0, v0x1a71a00_0;
    %vpi_call 2 35 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./tests/signextender.t.v";
    "./submodules/signextender.v";
