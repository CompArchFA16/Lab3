#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x27bdf40 .scope module, "testjump_shifter" "testjump_shifter" 2 8;
 .timescale -9 -12;
v0x27d2480_0 .var "clk", 0 0;
v0x27d2540_0 .var "dutpassed", 0 0;
v0x27d25e0_0 .net "extended_imm", 31 0, L_0x27d2890; 1 drivers
v0x27d2690_0 .var "immediate", 25 0;
v0x27d2770_0 .var "pc_bits", 3 0;
S_0x27be030 .scope module, "dut" "jump_shifter" 2 18, 3 9, S_0x27bdf40;
 .timescale -9 -12;
v0x279b1f0_0 .net *"_s0", 1 0, C4<00>; 1 drivers
v0x27d2290_0 .alias "extended_imm", 31 0, v0x27d25e0_0;
v0x27d2330_0 .net "immediate", 25 0, v0x27d2690_0; 1 drivers
v0x27d23d0_0 .net "pc_bits", 3 0, v0x27d2770_0; 1 drivers
L_0x27d2890 .concat [ 2 26 4 0], C4<00>, v0x27d2690_0, v0x27d2770_0;
    .scope S_0x27bdf40;
T_0 ;
    %delay 1000, 0;
    %load/v 8, v0x27d2480_0, 1;
    %inv 8, 1;
    %set/v v0x27d2480_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x27bdf40;
T_1 ;
    %set/v v0x27d2540_0, 1, 1;
    %set/v v0x27d2480_0, 1, 1;
    %movi 8, 5, 4;
    %set/v v0x27d2770_0, 8, 4;
    %set/v v0x27d2690_0, 1, 26;
    %delay 0, 0;
    %load/v 8, v0x27d25e0_0, 32;
    %movi 40, 1610612732, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_1.0, 4;
    %set/v v0x27d2540_0, 0, 1;
    %vpi_call 2 34 "$display", "Test Case 1 Failed: MSB in imm 1 implementation failed";
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 37 "$display", "Test Case 1 Passed";
T_1.1 ;
    %delay 2000, 0;
    %movi 8, 33554431, 26;
    %set/v v0x27d2690_0, 8, 26;
    %delay 2000, 0;
    %load/v 8, v0x27d25e0_0, 32;
    %movi 40, 1476395004, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_1.2, 4;
    %set/v v0x27d2540_0, 0, 1;
    %vpi_call 2 48 "$display", "Test Case 2 Failed: MSB in imm 0 implementation failed";
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 51 "$display", "Test Case 2 Passed";
T_1.3 ;
    %vpi_call 2 59 "$display", "dutpassed: %b", v0x27d2540_0;
    %delay 2000, 0;
    %vpi_call 2 60 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "jump_shifter.t.v";
    "./jump_shifter.v";
