#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2114f40 .scope module, "testjump_shifter" "testjump_shifter" 2 8;
 .timescale -9 -12;
v0x2127760_0 .var "clk", 0 0;
v0x2127820_0 .net "extended_imm", 31 0, L_0x2127ad0; 1 drivers
v0x21278d0_0 .var "immediate", 25 0;
v0x2127980_0 .var "pc_bits", 3 0;
S_0x2115030 .scope module, "dut" "jump_shifter" 2 17, 3 9, S_0x2114f40;
 .timescale -9 -12;
v0x20f20d0_0 .net *"_s0", 1 0, C4<00>; 1 drivers
v0x2127570_0 .alias "extended_imm", 31 0, v0x2127820_0;
v0x2127610_0 .net "immediate", 25 0, v0x21278d0_0; 1 drivers
v0x21276b0_0 .net "pc_bits", 3 0, v0x2127980_0; 1 drivers
L_0x2127ad0 .concat [ 2 26 4 0], C4<00>, v0x21278d0_0, v0x2127980_0;
    .scope S_0x2114f40;
T_0 ;
    %delay 1000, 0;
    %load/v 8, v0x2127760_0, 1;
    %inv 8, 1;
    %set/v v0x2127760_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x2114f40;
T_1 ;
    %set/v v0x2127760_0, 1, 1;
    %movi 8, 5, 4;
    %set/v v0x2127980_0, 8, 4;
    %set/v v0x21278d0_0, 1, 26;
    %delay 0, 0;
    %vpi_call 2 29 "$display", "  immediate               |     Ext      ";
    %vpi_call 2 30 "$display", "    %b      |   %b   ", v0x21278d0_0, v0x2127820_0;
    %movi 8, 33554431, 26;
    %set/v v0x21278d0_0, 8, 26;
    %delay 0, 0;
    %vpi_call 2 35 "$display", "  immediate               |     Ext      ";
    %vpi_call 2 36 "$display", "    %b      |   %b   ", v0x21278d0_0, v0x2127820_0;
    %delay 2000, 0;
    %vpi_call 2 38 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "jump_shifter.t.v";
    "./jump_shifter.v";
